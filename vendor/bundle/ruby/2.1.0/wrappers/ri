#!/usr/bin/env bash

if
  [[ -s "/Users/grantgarland/Projects/Self/my_cookbook/vendor/bundle/ruby/2.1.0/environment" ]]
then
  source "/Users/grantgarland/Projects/Self/my_cookbook/vendor/bundle/ruby/2.1.0/environment"
  exec ri "$@"
else
  echo "ERROR: Missing RVM environment file: '/Users/grantgarland/Projects/Self/my_cookbook/vendor/bundle/ruby/2.1.0/environment'" >&2
  exit 1
fi
